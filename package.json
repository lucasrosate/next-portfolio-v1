{
  "name": "myportfoliov2",
  "version": "0.1.0",
  "private": true,
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": false
            }
          ]
        ]
      }
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "gh-pages -d next build"
  },
  "dependencies": {
    "autoprefixer": "^10.2.3",
    "next": "10.0.5",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.7.0",
    "node": "^15.4.0",
    "postcss": "^8.2.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "^4.1.0",
    "styled-components": "^5.2.1",
    "ts-lint": "^4.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@types/typescript": "^2.0.0",
    "gh-pages": "^3.1.0"
  }
}
